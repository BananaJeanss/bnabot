{
    "display_information": {
        "name": "BnaBot",
        "description": "BananaJeans's Personal Bot",
        "background_color": "#002a42"
    },
    "features": {
        "bot_user": {
            "display_name": "BnaBot",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/bnaping",
                "description": "Ping Pong",
                "should_escape": false
            },
            {
                "command": "/bnarateuser",
                "description": "Compliment or Roast yourself",
                "usage_hint": "[compliment/roast]",
                "should_escape": false
            },
            {
                "command": "/bnanurburgring",
                "description": "Fetches NÃ¼rburgring Data",
                "should_escape": false
            },
            {
                "command": "/bnaabout",
                "description": "See information about BnaBot",
                "should_escape": false
            },
            {
                "command": "/bnaweekly",
                "description": "BananaJeans's Weekly Stats",
                "should_escape": false
            },
            {
                "command": "/bnajoinpersonal",
                "description": "Adds you to BananaJeans's personal channel",
                "should_escape": false
            },
            {
                "command": "/bnaneststatus",
                "description": "Shows my account stats + some nest stats",
                "should_escape": false
            },
            {
                "command": "/bnauptime",
                "description": "Checks Uptime Kuma service statuses",
                "should_escape": false
            },
            {
                "command": "/bnaexecute",
                "description": "[BnaJns only] Execute a comamnd on Nest",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:read",
                "channels:write.invites",
                "commands",
                "groups:read",
                "mpim:read",
                "users.profile:read",
                "users:read",
                "chat:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "member_joined_channel"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}